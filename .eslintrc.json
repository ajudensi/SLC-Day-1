{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "consistent-return": 0,
        "strict": 0,
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "prefer-rest-params": 0,
        "prefer-template": 0
    }
}